programa
{
	
	funcao inicio()
	{
		cadeia nome
		escreva("Digite o nome do usuário:")
		leia(nome)
		se(nome == "Barbara"){
			escreva("Bem vinda ", nome)
			}
			senao{
				escreva("Usuário não cadastrado!")
				}
	}
}
